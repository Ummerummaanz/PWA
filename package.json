{
  "name": "oasis-feature-header-toolbar",
  "version": "22.6.0",
  "description": "header-toolbar",
  "license": "UNLICENSED",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist/app"
  ],
  "repository": {
    "type": "git",
    "url": "https://stash.synchronoss.net/scm/oasis/feature-header-toolbar.git"
  },
  "scripts": {
    "init": "oasis-os-cli init",
    "compile": "oasis-os-cli build",
    "serve": "oasis-os-cli serve",
    "lint": "eslint {src,__tests__,__mocks__}/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix {src,__tests__,__mocks__}/**/*.{ts,tsx}",
    "test": "jest --coverage --ci --maxWorkers=2",
    "testScan": "yarn test && yarn sonar",
    "sonar": "node sonar-scan.js",
    "prepublishOnly": "npm run test",
    "prepare": "husky install"
  },
  "publishConfig": {
    "registry": "https://npm-write.synchronoss.net/repository/npm-sncr-private/"
  },
  "featureApp": {
    "port": "4031",
    "env": [
      "CONTENTFUL_TOKEN",
      "CONTENTFUL_SPACE",
      "CONTENTFUL_ENVIRONMENT",
      "DEV_SERVER_PROXY_TARGETS",
      "DEV_SERVER_PROXY_CONTEXTS"
    ],
    "shares": {
      "oasis-feature-api": {
        "singleton": true,
        "version": "0.2.3"
      }
    }
  },
  "dependencies": {
    "@ionic/react": "^5.6.9",
    "@ionic/react-router": "^5.6.9",
    "contentful": "^8.3.7",
    "oasis-feature-api": "^0.2.3",
    "oasis-os-contentful": "^2.2.34",
    "oasis-os-react": "^1.6.8",
    "oasis-os-theming": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^12.1.1",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/config-lerna-scopes": "^11.0.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "^10.5.3",
    "oasis-auth": "^0.0.12",
    "oasis-os-build-scripts": "^1.7.0",
    "prettier": "^2.3.1",
    "sonarqube-scanner": "2.8.1",
    "typescript": "^4.3.2",
    "whatwg-fetch": "^3.6.2"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "*.{ts,tsx,json,js,html,css}": [
      "prettier --write"
    ]
  }
}
